{
  "name": "donejs-amplify",
  "version": "0.0.0",
  "description": "An awesome DoneJS app",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donejs-user/donejs-amplify.git"
  },
  "author": {
    "name": "Matthew Phillips",
    "email": "matthew@matthewphillips.info",
    "url": "https://donejs.com"
  },
  "private": true,
  "scripts": {
    "test": "testee test.html --browsers firefox --reporter Spec",
    "start": "done-serve --port 8080",
    "develop": "done-serve --develop --port 8080",
    "build": "node build"
  },
  "main": "donejs-amplify/index.stache!done-autorender",
  "files": [
    "src"
  ],
  "keywords": [
    "",
    "donejs-app"
  ],
  "steal": {
    "main": "donejs-amplify/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register",
      "node_modules/steal-conditional/conditional"
    ],
    "plugins": [
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "/dist"
      }
    },
    "serviceBaseURL": ""
  },
  "dependencies": {
    "aws-amplify": "^1.1.29",
    "can-component": "^4.0.2",
    "can-connect": "^3.1.0",
    "can-define": "^2.0.1",
    "can-route": "^4.1.0",
    "can-route-pushstate": "^5.0.13",
    "can-set": "^1.5.1",
    "can-stache": "^4.1.0",
    "can-stache-route-helpers": "<2.0.0",
    "can-view-autorender": "^5.0.4",
    "can-zone": "^1.0.0",
    "done-autorender": "^2.0.0",
    "done-component": "^2.0.0",
    "done-css": "^3.0.2",
    "done-serve": "^3.3.1",
    "generator-donejs": "^3.4.0",
    "steal": "^2.2.1",
    "steal-less": "^1.2.2",
    "steal-stache": "^4.0.1"
  },
  "devDependencies": {
    "can-debug": "^2.0.7",
    "can-fixture": "^3.1.5",
    "donejs-cli": "^3.2.2",
    "funcunit": "^3.2.0",
    "steal-conditional": "^1.0.0",
    "steal-qunit": "^2.0.0",
    "steal-tools": "^2.2.2",
    "testee": "^0.9.1"
  },
  "license": "MIT"
}
